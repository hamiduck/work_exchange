<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.work_exchange">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>





        <activity android:name=".MainActivity"/>

        <activity android:name=".ui.personal.profile.ProfileActivity"/>

        <activity android:name=".ui.personal.profile.ProfileListActivity"/>

        <activity android:name=".ui.publish.profile.testUploadPage"/>

        <activity android:name=".ui.publish.profile.testShow"/>

        <activity android:name=".ui.publish.profile.UploadPage"/>

        <activity android:name=".ui.home.profile.ProductShowcase"/>

        <activity android:name=".ui.personal.record.RecordActivity"/>

        <activity android:name=".ui.personal.evaluation.EvaluationActivity"/>



        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>